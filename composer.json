{
    "name": "lukaszaleckas/laravel-correlation-id",
    "type": "library",
    "autoload": {
        "psr-4": {
            "LaravelCorrelationId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCorrelationId\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.15",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan-mockery": "^1.1.2",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6",
        "larastan/larastan": "^2.8.1",
        "orchestra/testbench": "^v7.40.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCorrelationId\\Providers\\CorrelationIdServiceProvider"
            ]
        }
    }
}
