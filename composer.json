{
    "name": "lukaszaleckas/laravel-correlation-id",
    "type": "library",
    "autoload": {
        "psr-4": {
            "LaravelCorrelationId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCorrelationId\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.3.30"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.49"
    },
    "require-dev": {
        "fakerphp/faker": "^1.15",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan-mockery": "^0.12.14",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "^6.20"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCorrelationId\\Providers\\CorrelationIdServiceProvider"
            ]
        }
    }
}
